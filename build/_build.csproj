<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <NoWarn>CS0649;CS0169</NoWarn>
    <NukeTelemetryVersion>1</NukeTelemetryVersion>
    <PreserveCompilationContext>true</PreserveCompilationContext>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Nuke.Common" Version="6.3.0" />
    <PackageReference Include="RazorLight" Version="2.3.0" />
    <PackageReference Include="RxBim.Nuke.AutoCAD" Version="2.1.0-dev003" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Models\Result.cshtml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>
  
  <ItemGroup>
    <PackageDownload Include="NuGet.CommandLine" Version="[6.3.1]" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\RxBim.AutocadTestFramework.Console\RxBim.AutocadTestFramework.Console.csproj" />
  </ItemGroup>

</Project>
