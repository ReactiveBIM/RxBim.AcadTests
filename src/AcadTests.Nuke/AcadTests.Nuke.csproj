<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="BimLab.Nuke" Version="2024.2.1" />
        <PackageReference Include="Nuke.Common" Version="8.0.0" />
        <PackageReference Include="RazorLight" Version="2.3.0" />
    </ItemGroup>

    <ItemGroup>
        <EmbeddedResource Include="Models\Result.cshtml">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </EmbeddedResource>
    </ItemGroup>

    <PropertyGroup>
        <Description>Nuke utils for runing tests.</Description>
        <PackageId>RxBim.AcadTests.Nuke</PackageId>
        <IsPackable>true</IsPackable>
    </PropertyGroup>
</Project>
